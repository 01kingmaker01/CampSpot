                <div class="modal-content">
                  <div class="modal-header">
                    <p class="modal-title" id="exampleModalLabel">
                      <strong><%= currentUser.username %></strong>, do you
                      really want to Log Out?
                    </p>
                  </div>
                  <div class="modal-body card-subtitle">
                    You will be not able to use some features like add
                    campground, add comment and many more...
                  </div>
                  <div class="modal-footer">
                    <button
                      type="button"
                      class="btn rounded-pill btn-secondary"
                      data-dismiss="modal"
                    >
                      Close
                    </button>
                    <a
                      href="/logout"
                      type="button"
                      class="btn rounded-pill btn-danger"
                      >logout</a
                    >
                  </div>
                </div>
              </div>
            </div>

            <% } %>
          </ul>
        </div>
      </div>
    </nav>

    <% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error)
    {%>
    <div class="alert alert-warning mb-0 selector" role="alert">
      <%= error.msg %>
    </div>
    <% }); %> <% } %> <% if(success_msg != ''){ %>
    <div class="alert alert-success mb-0 selector" role="alert">
      <%= success_msg %>
    </div>
    <% } %> <% if(error_msg != ''){ %>
    <div class="alert alert-danger mb-0 selector" role="alert">
      <%= error_msg %>
    </div>
    <% } %> <% if(error != ''){ %>
    <div class="alert alert-danger mb-0 selector" role="alert">
      <%= error %>
    </div>
    <% } %> <% if (success && success.length>0) { %>
    <div class="alert alert-success mb-0 selector" role="alert">
      <%= success %>
    </div>
    <% } %>
    <!-- <%= user %> -->
    <div class="container mt-5">
      <div class="row">
        <div class="col-12 col-md-4 col-lg-4 col-xl-3">
          <div id="profile" class="card mb-4 text-center">
            <div class="bg-img-profile"></div>
            <span
              id="pic"
              style="background-image: url('<%= user.avatar %>')"
            ></span>
            <div id="body">
              <h1 id="name"><%= user.username %></h1>
              <h1 id="username"><%= user.name %></h1>
              <p class="pl-4 pr-4">
                Lorem ipsum dolor sit amet consectetur adipisicing elit. Ipsam,
                ut. Perspiciatis doloribus praesentium voluptatum, tempore saepe
                neque eaque commodi dicta fuga earum a odio culpa quisquam,
                nihil sint harum reiciendis!
              </p>
            </div>
          </div>
        </div>

        <div class="col-12 col-md-8 col-lg-8 col-xl-9">
          <%if (!campInfo) {%>
          <h3>No CampGrounds to Display!!!</h3>
          <%}%> <% for( let i = 0; i < campInfo.length; i++ ) { %>

          <div class="card mb-4">
            <img
              class="card-img-top border rounded"
              src="<%= campInfo[i].img %>"
            />
            <div class="card-body text-center">
              <div class="mb-3">
                <a href="/campgrounds/<%=campInfo[i]._id%>" class="text-dark">
                  <h4 class="card-title d-inline"><%=campInfo[i].name%></h4>
                </a>
              </div>
            </div>
          </div>

          <% } %>
        </div>
      </div>
    </div>

    <%- include('./partials/footer') %>
  </body>
</html>
